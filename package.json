{
    "name": "@rabrennie/sveltekit-auth",
    "version": "0.0.7",
    "homepage": "https://github.com/Rabrennie/sveltekit-auth",
    "description": "Auth library for SvelteKit supporting OpenID/OAuth providers",
    "license": "MIT",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build && npm run package",
        "preview": "vite preview",
        "package": "svelte-kit sync && svelte-package && publint",
        "prepublishOnly": "npm run package",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "test": "vitest",
        "lint": "prettier --plugin-search-dir . --check . && eslint .",
        "format": "prettier --plugin-search-dir . --write ."
    },
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/index.d.ts",
            "svelte": "./dist/index.js",
            "import": "./dist/index.js"
        },
        "./session": {
            "types": "./dist/SessionStrategies/index.d.ts",
            "import": "./dist/SessionStrategies/index.js"
        },
        "./providers": {
            "types": "./dist/AuthProviders/index.d.ts",
            "import": "./dist/AuthProviders/index.js"
        },
        "./helpers": {
            "types": "./dist/Helpers/index.d.ts",
            "import": "./dist/Helpers/index.js"
        },
        "./client": {
            "types": "./dist/Client/index.d.ts",
            "import": "./dist/Client/index.js"
        }
    },
    "files": [
        "dist",
        "!dist/**/*.test.*",
        "!dist/**/*.spec.*"
    ],
    "peerDependencies": {
        "@sveltejs/kit": "^1.5.0",
        "svelte": "^3.54.0"
    },
    "devDependencies": {
        "@sveltejs/adapter-auto": "^2.0.0",
        "@sveltejs/package": "^2.0.0",
        "@types/jsonwebtoken": "^9.0.1",
        "@typescript-eslint/eslint-plugin": "^5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "eslint": "^8.28.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-jsdoc": "^43.1.1",
        "eslint-plugin-svelte3": "^4.0.0",
        "prettier": "^2.8.0",
        "prettier-plugin-svelte": "^2.8.1",
        "publint": "^0.1.9",
        "svelte": "^3.54.0",
        "svelte-check": "^3.0.1",
        "tslib": "^2.4.1",
        "typescript": "^5.0.0",
        "vite": "^4.2.0",
        "vitest": "^0.29.8",
        "vitest-mock-extended": "^1.1.3"
    },
    "svelte": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "type": "module",
    "dependencies": {
        "jose": "^4.14.4",
        "openid-client": "^5.4.1"
    },
    "typesVersions": {
        "*": {
            "*": [
                "./dist/index.d.ts"
            ],
            "session": [
                "./dist/SessionStrategies/index.d.ts"
            ],
            "providers": [
                "./dist/AuthProviders/index.d.ts"
            ],
            "helpers": [
                "./dist/Helpers/index.d.ts"
            ],
            "client": [
                "./dist/Client/index.d.ts"
            ]
        }
    }
}
